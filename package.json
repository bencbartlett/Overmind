{
  "name": "Overmind",
  "version": "0.6.0",
  "description": "Overmind Screeps AI",
  "author": "Ben Bartlett",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencbartlett/Overmind.git"
  },
  "homepage": "https://github.com/bencbartlett/Overmind#readme",
  "bugs": {
    "url": "https://github.com/bencbartlett/Overmind/issues"
  },
  "scripts": {
    "build": "tsc -p .",
    "lint": "tslint \"src/**/*.ts\"",
    "clean": "rm -rf tsc-out && rm -rf dist",
    "obfuscate": "tsc && javascript-obfuscator tsc-out/Overmind.js --output src/Overmind_obfuscated.js --compact false --self-defending true --string-array true --string-array-threshold 1 --string-array-encoding rc4 && javascript-obfuscator tsc-out/assimilation/Assimilator.js --output src/assimilation/Assimilator_obfuscated.js --compact false --self-defending true --string-array true --string-array-threshold 1 --string-array-encoding rc4",
    "compile": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-seasonal": "rollup -c --environment DEST:seasonal",
    "push-pserver": "rollup -c --environment DEST:pserver",
    "push-screepsplus": "rollup -c --environment DEST:screepsplus",
    "test": "npm run clean && npm run build"
  },
  "engines": {
    "node": "^20.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/lodash": "3.10.2",
    "@types/node": "^20.11.16",
    "@types/screeps": "^3.3.7",
    "javascript-obfuscator": "^4.1.0",
    "rollup": "^3.27.1",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-screeps": "^1.0.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.25.7",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^1.2.11",
    "columnify": "^1.6.0",
    "onnxjs": "^0.1.6",
    "source-map": "0.7.3"
  }
}
