{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "forin": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-ordering": [
      false
    ],
    "no-console": [
      false
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
